# 2Ô∏è‚É£ Moyenne alg√©brique et g√©om√©trique
# -------------------------------
import math

def algebrique(L):
    return sum(L) / len(L) if L else 0

def geometrique(L):
    produit = 1
    for x in L:
        produit *= x
    return produit**(1/len(L)) if L else 0

liste = [2, 4.5, 6, 8, 10]
print("\nLa moyenne alg√©brique est :", algebrique(liste))
print("La moyenne g√©om√©trique est :", geometrique(liste))

# -------------------------------
# 3Ô∏è‚É£ Ensembles et comprehension
# -------------------------------
K = ['A', 'B', 'C']
X = [1, 2, 3, 4, 5, 6]

def P(x):
    return x % 2 == 0  # condition : x pair

S = {(k, x) for k in K for x in X if P(x)}
print("\nEnsemble des couples filtr√©s :", S)

x_values = [x for (_, x) in S]
moyenne = algebrique(x_values) if x_values else 0
print("Moyenne alg√©brique des x filtr√©s :", moyenne)

