    "def factorielle_recursive(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    return n * factorielle_recursive(n - 1)\n",
    "\n",
    "# Exemple\n",
    "factorielle_recursive(5)"
   ]
