    "def factorielle_iterative(n):\n",
    "    resultat = 1\n",
    "    for i in range(1, n + 1):\n",
    "        resultat *= i\n",
    "    return resultat\n",
    "\n",
    "# Exemple\n",
    "factorielle_iterative(5)"
   ]
