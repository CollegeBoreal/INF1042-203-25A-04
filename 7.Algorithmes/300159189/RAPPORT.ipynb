  {
   "cell_type": "code",
   "metadata": {},
   "source": [
    "def factorielle_iterative(n):\n",
    "    resultat = 1\n",
    "    for i in range(1, n + 1):\n",
    "        resultat *= i\n",
    "    return resultat\n",
    "\n",
    "# Exemple\n",
    "factorielle_iterative(5)"
   ],
   "execution_count": null,
   "outputs": []
  },
  {
   "cell_type": "code",
   "metadata": {},
   "source": [
    "def factorielle_recursive(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    return n * factorielle_recursive(n - 1)\n",
    "\n",
    "# Exemple\n",
    "factorielle_recursive(5)"
   ],
   "execution_count": null,
   "outputs": []
